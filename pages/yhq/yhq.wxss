.swiperBox{
margin-top: 20rpx;
overflow: hidden;
zoom:1;

}
.swiper-tab{
  padding:0 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  text-align: center;  
  padding-bottom: 10rpx;
}
.xian{
  height: 15rpx;
  width: 100%;
  background-color:#eee;
  margin-bottom: 20rpx;
}
.swiper-tab-item{
  flex:1;
  color: #000;
  font-size: 26rpx;
}
.active{
  color: #ffc257;
  border-bottom: 2rpx solid #ffc257;
  font-weight: bold;
}
swiper{
  text-align: center;
  overflow: hidden;
  zoom:1;
}
.yhBox{
  width:632rpx;
  margin: 0 auto;
}
.yhBox .nolist{
  width:680rpx;
  height: 540rpx;
}
.yhqbg{
  width:632rpx;
  height: 267rpx;
  background:url(https://img.5211game.com/5211/Act/2020/yykingWx/images/quan.png);
  background-size: cover;
  position:relative;
  margin-bottom: 10rpx;
}
.yhqbg .left{
  float: left;
  width:300rpx;
  height: 267rpx;
  text-align: center;
  line-height: 267rpx;
  color: #ffd38f;
  font-size: 80rpx;
  font-weight: bold;
}
.hasUseBg .left{
  color: #b9b9b9;
}
.yhqbg .right{
  float: left;
  width:332rpx;
  height: 235rpx;
  font-size: 26rpx;
  color: #ffd38f;
  padding-top: 32rpx;
}
.hasUseBg .right{
  color: #b9b9b9;
}
.yhqbg .right .wt{
  font-weight: bold;
}
.yhqbg .right view{
  margin-bottom: 12rpx;
}
.yhqbg .right .tip{
  padding: 0 10rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background-color:#ffd38f;
  color: #323232;
}
.hasUseBg .right .tip{
  color: #323232;
   background-color:#b9b9b9;
}
.yhqbg .right view.title{
  color: #fff;
  font-size: 22rpx;
  margin-top: 10rpx;
}
.hasUseBg .right view.title{
color: #b9b9b9;
}
